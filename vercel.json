{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "devCommand": "npm start",
  "env": {
    "REACT_APP_BASE_URL": "https://admin.disruptorsmedia.com"
  },
  "build": {
    "env": {
      "REACT_APP_BASE_URL": "https://admin.disruptorsmedia.com"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}